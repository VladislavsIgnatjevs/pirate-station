{
  "name": "Pirate Station",
  "description": "Chrome app that gives ability to watch movies online for free",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "browser",
    "notifications",
    "webview",
    "fullscreen",
    "https://*/*",
    "http://*/*"
    ],
  "icons": { "16": "pir-16.png", "128": "pir-128.png" }
}