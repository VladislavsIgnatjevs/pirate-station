{
  "name": "Pirate Station",
  "description": "Viewing list of mp4 files from source",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "web_accessible_resources": ["*.png", ["*.jpg"]],
  "permissions": [
    "browser",
    "notifications",
    "https://*/*"
    ],
  "icons": { "16": "pir-16.png", "128": "pir-128.png" }
}